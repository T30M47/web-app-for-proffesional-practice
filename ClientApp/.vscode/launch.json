{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "localhost (Chrome)",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "userDataDir": false,
      "skipFiles": [ "<node_internals>/**", "${workspaceFolder}/node_modules/**/*.js" ]
    },
    {
      "type": "edge",
      "request": "launch",
      "name": "localhost (Edge)",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [ "<node_internals>/**", "${workspaceFolder}/node_modules/**/*.js" ]
    }
  ]
}
